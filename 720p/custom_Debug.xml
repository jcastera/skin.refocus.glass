<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="98">
	<zorder>20</zorder>
	<visible>!Window.IsVisible(startup) + Skin.HasSetting(dev.enable)</visible>
	<controls>
		<!--grid-->
		<control type="image">
			<include>screen_Dimensions</include>
			<texture>overlays/debuggrid720.png</texture>
			<colordiffuse>Color3</colordiffuse>
		</control>
		<!--info-->
		<control type="grouplist">
			<posx>10</posx>
			<bottom>6</bottom>
			<height>74</height>
			<itemgap>0</itemgap>
			<control type="label" id="1">
				<label>[B]$VAR[debug_WindowName]$INFO[Container.Viewmode, - ][/B]</label>
				<font>semibold</font>
				<height>18</height>
				<textcolor>yellow</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<control type="label" id="2">
				<label>$INFO[System.CurrentControl]</label>
				<font>semibold_Small</font>
				<height>18</height>
				<textcolor>yellow</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<control type="label" id="3">
				<label>$INFO[Container.FolderName,, - ]$INFO[Container.FolderPath]</label>
				<font>semibold_Small</font>
				<height>18</height>
				<textcolor>yellow</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<control type="label" id="4">
				<label>$INFO[System.ScreenResolution]</label>
				<font>semibold_Small</font>
				<height>18</height>
				<textcolor>yellow</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
		</control>
	</controls>
</window>