<?xml version="1.0" encoding="UTF-8"?>
<window>
	<coordinates>
		<system>1</system>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<zorder>1</zorder>
	<controls>
		<control type="group">
			<visible>![Window.IsActive(startup) | Window.IsActive(movieinformation) | Window.IsActive(musicinformation)]</visible>
			<control type="label" id="401">
				<right>70</right>
				<include>toast_Top</include>
			</control>
			<control type="label" id="402">
				<right>70</right>
				<include>toast_Bottom</include>
			</control>
			<!--default-->
			<control type="image" id="400">
				<include>toast_Icon</include>
				<texture>toast/info.png</texture>
			</control>
			<!--info-->
			<control type="image" id="403">
				<include>toast_Icon</include>
				<visible>false</visible>
				<texture>toast/info.png</texture>
			</control>
			<!--warning-->
			<control type="image" id="404">
				<include>toast_Icon</include>
				<visible>false</visible>
				<texture>toast/warning.png</texture>
			</control>
			<!--error-->
			<control type="image" id="405">
				<include>toast_Icon</include>
				<visible>false</visible>
				<texture>toast/error.png</texture>
			</control>
		</control>
