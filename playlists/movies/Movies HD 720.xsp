<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Movies HD 720</name>
    <match>all</match>
    <rule field="videoresolution" operator="is">
        <value>720</value>
    </rule>
    <group>none</group>
    <order direction="ascending">sorttitle</order>
</smartplaylist>
